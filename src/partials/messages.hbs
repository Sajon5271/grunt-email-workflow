<table class="corsol-main py-24 pt-10 default-font-size border-space-8" style="box-sizing: border-box; color: #4d5052; font-size: 16px; line-height: 20px; letter-spacing: 0.7%; padding-top: 10px; padding-bottom: 24px; border-spacing: 8px; border-collapse: separate !important; mso-table-lspace: 0pt; mso-table-rspace: 0pt; width: 100%;" width="100%">
  {{#each corsol.messages}}
  <tr>
    <td class="mt-8 p-16 message" style="box-sizing: border-box;color: #212121;font-family: 'Calibri', 'Helvetica Neue', Helvetica, Arial,'Lucida Grande', sans-serif;font-size: 16px;line-height: 140%;font-style: normal;font-weight: 400;vertical-align: top;padding: 16px;border-radius: 8px;background-color: #f1f5f9;" valign="top" bgcolor="#f1f5f9">
      <table style="box-sizing: border-box;color: #212121;border-collapse: separate !important;mso-table-lspace: 0pt;mso-table-rspace: 0pt;width: 100%;" width="100%">
        <tr>
          <td class="circle bg-white square-36 center-text bold-700 vertical-center" style="box-sizing: border-box;color: #212121;font-family: 'Calibri', 'Helvetica Neue', Helvetica, Arial, 'Lucida Grande', sans-serif;font-size: 16px;line-height: 140%;font-style: normal;font-weight: 700;vertical-align: middle;width: 36px;height: 36px;border-radius: 10000px;background-color: white;text-align: center;" width="36" valign="middle" bgcolor="white" align="center">
            {{#if this.profile_pic}}
            <img src={{this.profile_pic}} style="-ms-interpolation-mode: bicubic;max-width: 100%;" />
            {{else}}
            <span>
              {{this.sender_inital}}
            </span>
            {{/if}}
          </td>
          <td class="vertical-center bold-700 message-sender" style="box-sizing: border-box;color: #212121;font-family: 'Calibri', 'Helvetica Neue', Helvetica,  Arial, 'Lucida Grande', sans-serif;font-size: 16px;line-height: 140%;font-style: normal;font-weight: 700;vertical-align: middle;padding-left: 16px;padding-top: 8px;padding-right: 8px;padding-bottom: 8px;" valign="middle">
            <span>{{this.sender}}
              <span class="dot" style="display: inline-block;width: 4px;height: 4px;border-radius: 5px;background: #bdbdbd;margin-left: 6px;margin-right: 6px;"></span>
              <span class="shrunk-font-size disabled-text bold-400" style="font-weight: 400;font-size: 14px;line-height: 20px;letter-spacing: 0.7%;color: #979797;">{{this.time}}</span>
            </span>
          </td>
        </tr>
        <tr>
          <td></td>
          <td class="medium-emphasis message-content" style="box-sizing: border-box;font-family: 'Calibri', 'Helvetica Neue', Helvetica, Arial, 'Lucida Grande', sans-serif;font-size: 16px;line-height: 140%;font-style: normal;font-weight: 400;vertical-align: top;color: #616161;padding-left: 16px;padding-right: 8px;" valign="top">{{this.msg}}<br />
            {{#each this.files}}
            <span>[File]</span>
            {{/each}}
          </td>
        </tr>
      </table>
    </td>
  </tr>
  {{/each}}
</table>